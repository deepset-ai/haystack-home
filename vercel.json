{
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "trailingSlash": false,

  "redirects": [
    {
      "source": "/old-page",
      "destination": "/tutorials",
      "permanent": true
    }
  ]
}
