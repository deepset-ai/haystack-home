{
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "trailingSlash": false,

  "redirects": [
    {
      "source": "/docs",
      "destination": "https://docs.haystack.deepset.ai/docs",
      "permanent": true
    }
  ]
}
